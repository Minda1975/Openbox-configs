set winname title
set winfmt %n %s %t
set msgwait 3
set font "Monospace-8"
set fgcolor #C898DC
set bgcolor #000000
set winliststyle column
set rudeness 12
set warp on
addhook key banish
set border 0 
set padding 0 0 0 0
set barpadding 0 0 
unbind c 
bind c exec urxvt
definekey top s-F1 exec ~/.scripts/shutdown.sh




