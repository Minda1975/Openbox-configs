# Set the prefix key to that of screen's default
set winname title
set winfmt %n %s %t
set bgcolor black
set fgcolor #4ca04c
msgwait 3
set font -*-fixed-*-*-*-*-9-*-*-*-*-*-*-*
set winliststyle column
rudeness 12
warp on
addhook key banish
set border 0 
set padding 0 0 0 0
set barpadding 0 0 
exec rpws init 4 -a -k
escape C-z
source .ratpoisonkeys


