# Set the prefix key to that of screen's default
set bgcolor #461B7E
set fgcolor #E3E4FA
set border 0 
set padding 0 0 0 0
set barpadding 0 0 
exec rpws init 4 -a -k
escape C-z
source .ratpoisonkeys


