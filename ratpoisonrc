# Set the prefix key to that of screen's default
set font "Anonymous Pro-11"
set bgcolor #312e39
set fgcolor #c0a79a
set border 0 
set padding 0 0 0 0
set barpadding 0 0 
exec rpws init 4 -a -k
escape C-z
source .ratpoisonkeys


